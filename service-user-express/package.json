{
  "name": "service-user-express",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist/ && tsc",
    "dev": "cross-env NODE_ENV='development' nodemon bin/run.ts",
    "test": "cross-env NODE_ENV='development' mocha -r ts-node/register --watch-extensions ts --colors _test_/*.ts --reporter mochawesome --timeout 15000 --exit",
    "pre-loading": "ts-node loading/generate-users.ts",
    "stress:register": "artillery run loading/config.yml --output loading/test-register.json",
    "stress:login": "artillery run loading/miscelaneas-user.yml --output loading/test-login.json",
    "stress:register-report": "artillery report --output loading/report-register.html loading/test-register.json",
    "stress:login-report": "artillery report --output loading/login-report.html loading/test-login.json",
    "start:pm2": "make build && cross-env NODE_ENV='development' pm2 start ./dist/bin/run.js --name='sue' --watch --",
    "start:pm2-cluster": "make build && cross-env NODE_ENV='development' pm2 start -i 6 ./dist/bin/run.js --name='sue'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/config": "^0.0.41",
    "@types/pino": "^7.0.5",
    "@types/pino-multi-stream": "^5.1.3",
    "bcrypt": "^5.0.1",
    "config": "^3.3.7",
    "connect-memcached": "^1.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "memcached": "^2.2.2",
    "mongoose": "^6.2.4",
    "node-fetch": "^3.2.3",
    "nodemailer": "^6.7.3",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0",
    "pino": "^7.8.0",
    "pino-multi-stream": "^6.0.0",
    "pino-tee": "^0.3.0",
    "pm2": "^5.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.0",
    "@types/chai-http": "^4.2.0",
    "@types/connect-memcached": "^1.0.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/memcached": "^2.2.7",
    "@types/mocha": "^9.1.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.21",
    "@types/node-fetch": "^2.6.1",
    "@types/objects-to-csv": "^1.3.1",
    "@types/passport": "^1.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "artillery": "^2.0.0-13",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "eslint": "^8.10.0",
    "mocha": "^9.2.1",
    "mochawesome": "^7.1.1",
    "nodemon": "^2.0.15",
    "objects-to-csv": "^1.3.6",
    "supertest": "^6.2.2",
    "ts-node": "^10.6.0",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.6.2"
  }
}
